{
  "name": "stackedit",
  "version": "5.15.4",
  "description": "Free, open-source, full-featured Markdown editor",
  "bugs": {
    "url": "https://github.com/benweet/stackedit/issues"
  },
  "license": "Apache-2.0",
  "author": "Benoit Schweblin",
  "main": "dist/style.css",
  "scripts": {
    "build": "webpack --config build/webpack.style.conf.js",
    "major": "npm version major -m \"Tag v%s\"",
    "minor": "npm version minor -m \"Tag v%s\"",
    "patch": "npm version patch -m \"Tag v%s\"",
    "postversion": "git push origin master --tags && npm publish"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "devDependencies": {
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^1.1.11",
    "katex": "^0.13.0",
    "node-sass": "^4.0.0",
    "normalize-scss": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "vue-style-loader": "^4.1.0",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  }
}
